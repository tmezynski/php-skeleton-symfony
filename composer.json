{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.3",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-bcmath": "*",
    "doctrine/doctrine-bundle": "^2.11",
    "doctrine/doctrine-migrations-bundle": "^3.3",
    "doctrine/orm": "^2.17",
    "ramsey/uuid": "^4.7",
    "symfony/console": "6.4.*",
    "symfony/doctrine-messenger": "6.4.*",
    "symfony/dotenv": "6.4.*",
    "symfony/flex": "^2",
    "symfony/framework-bundle": "6.4.*",
    "symfony/messenger": "6.4.*",
    "symfony/monolog-bundle": "^3.10",
    "symfony/property-access": "6.4.*",
    "symfony/routing": "6.4.*",
    "symfony/runtime": "6.4.*",
    "symfony/serializer": "6.4.*"
  },
  "require-dev": {
    "behat/behat": "^3.13",
    "friends-of-behat/symfony-extension": "^2.4",
    "friendsofphp/php-cs-fixer": "^3.58",
    "infection/infection": "^0.27.9",
    "phpstan/phpstan": "^1.10",
    "phpunit/phpunit": "^10.5",
    "qossmic/deptrac-shim": "^1.0"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/flex": true,
      "symfony/runtime": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "infection/extension-installer": true
    },
    "sort-packages": true,
    "process-timeout": 0
  },
  "autoload": {
    "files": [
      "src/Kernel.php"
    ],
    "psr-4": {
      "SharedKernel\\": "src/SharedKernel"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Unit\\": "test/Unit",
      "Integration\\": "test/Integration",
      "Utils\\": "test/Utils"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts",
      "@migrations"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "test:php-stan": [
      "APP_ENV=test php -d xdebug.mode=off vendor/bin/phpstan analyse -c stan.neon --no-progress --no-interaction --no-ansi"
    ],
    "test:cs-fixer": [
      "APP_ENV=test php -d xdebug.mode=off vendor/bin/php-cs-fixer fix --config=php-cs-fixer.php --dry-run --diff --allow-risky=yes"
    ],
    "test:cs-fixer:fix": [
      "APP_ENV=test php -d xdebug.mode=off vendor/bin/php-cs-fixer fix --config=php-cs-fixer.php --diff --allow-risky=yes --quiet"
    ],
    "test:deptrac": [
      "APP_ENV=test php -d xdebug.mode=off vendor/bin/deptrac analyse --config-file=deptrac.yml --cache-file=var/.deptrac.cache --fail-on-uncovered --report-uncovered"
    ],
    "test:unit": [
      "APP_ENV=test php -d xdebug.mode=off vendor/bin/phpunit --configuration=phpunit.xml --testsuite=unit --no-coverage"
    ],
    "test:integration": [
      "APP_ENV=test php -d xdebug.mode=off vendor/bin/phpunit --configuration=phpunit.xml --testsuite=integration --no-coverage"
    ],
    "test:behat": [
      "APP_ENV=test php -d xdebug.mode=off vendor/bin/behat --config=behat.yml --strict --no-colors --format=progress"
    ],
    "test:mutation": [
      "APP_ENV=test php -d xdebug.mode=off vendor/bin/infection --configuration=infection.json"
    ],
    "test:coverage": [
      "APP_ENV=test php -d pcov.enabled=1 -d xdebug.mode=off vendor/bin/phpunit --configuration=phpunit.xml --testsuite=unit,integration"
    ],
    "cache:clear": [
      "rm -rf var/cache",
      "@auto-scripts"
    ],
    "migrations": [
      "bin/console doctrine:migrations:migrate --no-interaction --allow-no-migration --quiet"
    ],
    "migrations-test": [
      "APP_ENV=test bin/console doctrine:migrations:migrate --no-interaction --allow-no-migration --quiet"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "6.4.*"
    }
  }
}
